<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PKHeXMAUI"
             x:Class="PKHeXMAUI.MiscRecords"
             Title="Records"
             x:Name="ThisView">
    <VerticalStackLayout>
        <local:comboBox x:Name="ComboRecord"/>
        <Entry x:Name="EntryRecordValue" Keyboard="Numeric"/>
        <Grid ColumnDefinitions="*,*,*">
            <Entry x:Name="EntryFameH" Keyboard="Numeric"/>
            <Entry x:Name="EntryFameM" Grid.Column="1" Keyboard="Numeric"/>
            <Entry x:Name="EntryFameS" Grid.Column="2" Keyboard="Numeric"/>
        </Grid>
    </VerticalStackLayout>
</ContentPage>